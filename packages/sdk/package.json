{
  "name": "@web3marketlabs/sdk",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./chains": {
      "import": "./dist/chains.js",
      "types": "./dist/chains.d.ts"
    },
    "./abi": {
      "import": "./dist/abi.js",
      "types": "./dist/abi.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@web3marketlabs/tsconfig": "workspace:*",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "viem": "^2.21.0"
  }
}
