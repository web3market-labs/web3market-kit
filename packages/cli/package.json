{
  "name": "@web3marketlabs/cli",
  "version": "0.3.0",
  "type": "module",
  "bin": {
    "web3market": "./dist/bin.js",
    "w3m": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@web3marketlabs/config": "workspace:*",
    "@web3marketlabs/codegen": "workspace:*",
    "@web3marketlabs/sdk": "workspace:*",
    "@web3marketlabs/components": "workspace:*",
    "commander": "^12.1.0",
    "@clack/prompts": "^0.8.0",
    "picocolors": "^1.1.0",
    "execa": "^9.5.0",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@web3marketlabs/tsconfig": "workspace:*",
    "@types/fs-extra": "^11.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
